<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="341.0" prefWidth="353.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="GAntiLag">
  <Pane layoutX="14.0" layoutY="14.0" prefHeight="317.0" prefWidth="327.0"
        style="-fx-border-radius: 20px; -fx-border-color: #888888;"/>
  <CheckBox fx:id="checkClickThrough" layoutX="28.0" layoutY="32.0" mnemonicParsing="false" onAction="#Click_through"
            text="Click Through"/>
  <CheckBox fx:id="checkHideSpeech" layoutX="28.0" layoutY="195.0" mnemonicParsing="false" onAction="#Click_through"
            text="Hide Speech"/>
  <CheckBox fx:id="checkHideShoutOut" layoutX="28.0" layoutY="220.0" mnemonicParsing="false" onAction="#Click_through"
            text="Hide Shout Out"/>
  <CheckBox fx:id="checkHideDance" layoutX="28.0" layoutY="246.0" mnemonicParsing="false" onAction="#Click_through"
            text="Hide Dance (not recommended)"/>
  <CheckBox fx:id="checkFreezeUsers" layoutX="28.0" layoutY="270.0" mnemonicParsing="false" onAction="#Click_through"
            text="Freeze Users (not recommended)"/>
  <CheckBox fx:id="checkIgnoreWhispers" layoutX="28.0" layoutY="295.0" mnemonicParsing="false" onAction="#Click_through"
            text="Ignore Whispers"/>
  <CheckBox fx:id="checkHideFloorItems" layoutX="28.0" layoutY="125.0" mnemonicParsing="false" onAction="#Click_through"
            text="Hide FloorItems"/>
  <CheckBox fx:id="checkHideWallItems" layoutX="28.0" layoutY="149.0" mnemonicParsing="false" onAction="#Click_through"
            text="Hide WallItems"/>
  <CheckBox fx:id="checkHideBubbles" layoutX="28.0" layoutY="171.0" mnemonicParsing="false" onAction="#Click_through"
            text="Hide Bubbles (You and the others)"/>
  <CheckBox fx:id="checkClickHide" layoutX="28.0" layoutY="58.0" mnemonicParsing="false"
            text="Double click for hide (Danger of crashing)"/>
  <CheckBox fx:id="checkDisableDouble" layoutX="28.0" layoutY="82.0" mnemonicParsing="false"
            onAction="#handleDoubleClick" text="Disable double click"/>
  <CheckBox fx:id="checkUsersToRemove" layoutX="28.0" layoutY="104.0" mnemonicParsing="false"
            text="Click users to remove"/>
</AnchorPane>
